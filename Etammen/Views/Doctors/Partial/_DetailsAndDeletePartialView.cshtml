@model Etammen.ViewModels.ClinicViewModel

@inject BusinessLogicLayer.Interfaces.IUnitOfWork _unitOfWork
@{
    string[] includes = { "Clinics", "DoctorReviews", "ApplicationUser" };
    // var doctorlist = new SelectList(await _unitOfWork.Doctors.GetAll(includes), "Id", "ApplicationUser.FirstName");
    var currentdoctor = await _unitOfWork.Doctors.FindBy(d => d.Id == Model.DoctorId, includes);

}
<div>
    <h4>Clinic Details</h4>
    <hr />
    <dl class="row" style="margin-left:5px">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            Doctor Name  @* @Html.DisplayNameFor(model => currentdoctor.ApplicationUser.FirstName) *@
        </dt>
        <dd class="col-sm-10">
            Dr. @Html.DisplayFor(model => currentdoctor.ApplicationUser.FirstName) @Html.DisplayFor(model => currentdoctor.ApplicationUser.LastName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.StreetAddress)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StreetAddress)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.governorate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.governorate)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.City)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Fees)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fees)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ExmainationDuration)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ExmainationDuration)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OpeningHour)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OpeningHour)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.ClosingHour)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ClosingHour)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OpeningDays)
